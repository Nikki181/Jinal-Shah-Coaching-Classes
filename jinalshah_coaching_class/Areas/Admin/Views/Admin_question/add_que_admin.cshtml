@using (Html.BeginForm("add_que", "Admin_question", FormMethod.Post, new { enctype = "multipart/form-data" }))
{
    @Html.ValidationSummary(true)
    
        <div class="panel panel-default">
                                        <div class="panel-heading">
                                            <h4 class="panel-title"><i>Select values from following</i></h4>
                                        </div>
                                        <div class="panel-body">
                           <div class="form-group">
							<label for="sem">Subject</label>
                                 <select name="sem" class="form-control input-sm" >
										<option>C</option>
                                     <option>C++</option>
                                     <option>Java</option>
                             </select></div>

                                <div class="form-group">
							<label for="sem">Exam Level</label>
                                 <select name="sem" class="form-control input-sm" >
										<option>Easy</option>
                                     <option>Medium</option>
                                     <option>Tough</option>
                             </select></div>

                                            <div class="form-group">
							<label for="sem">Exam Marks</label>
                                                 <input  name="txtlnm" type="text" class="form-control input-sm" placeholder="Enter marks">
                                 </div>

                                            <div class="form-group">
							<label for="sem">Exam Duration</label>
                                                  <input  name="txtlnm" type="text" class="form-control input-sm" placeholder="Enter exam duration">
                                </div>

                                             <div class="form-group">
							<label for="sem">Semester</label>
                                 <select name="sem" class="form-control input-sm" >
										<option>1</option>
                                        <option>2</option>
										<option>3</option>
										<option>4</option>
                                        <option>5</option>
                                 	    <option>6</option>
                                        <option>7</option>
										<option>8</option>
										<option>9</option>
                                        <option>10</option>
                             </select></div>
                              <!--//form-group-->
     
                               <hr />
                                 <div class="controls text-center">
                                       <h3>
                                         <input type="submit" value="Next" class="btn btn-theme"  name="submit" />
                                        </h3>
                                </div>
    
                    </div>
            </div>
}