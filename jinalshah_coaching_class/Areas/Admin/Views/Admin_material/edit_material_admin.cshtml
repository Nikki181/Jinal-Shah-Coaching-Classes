@using (Html.BeginForm("Edit","Admin_material",FormMethod.Post, new { enctype = "multipart/form-data" }))
{
    @Html.ValidationSummary(true)

   
    <fieldset>
        <legend>Material Edit</legend>

       
        <div>
            @Html.Label("Title:")
        </div>
        <div>
            @Html.EditorFor(model => model.material_name)
            @Html.ValidationMessageFor(model => model.material_name)
        </div>

         
        <div class="editor-field">
            <label for="name">Upload Here:</label>
            <input  type="file" name="fileuploadDemo"  id="fileupld" class="form-control input-sm" />
        </div>


        <div>
            @Html.Label("Select Subject:")
        </div>
        <div>
           	<select name="type_mat"  required class="form-control input-sm" >
										<option>CG</option>
                                        <option>SS</option>
										<option>DFS</option>
                                        <option>DC</option>
                                        <option>JAVA</option>
									
			</select>
								
            @Html.ValidationMessageFor(model => model.type_of_material)
        </div>


        <p>
            <input type="submit" value="Save" />
        </p>
    </fieldset>
}


@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
