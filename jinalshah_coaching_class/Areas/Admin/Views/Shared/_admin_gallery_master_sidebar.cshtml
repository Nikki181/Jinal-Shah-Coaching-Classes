                                List<jinalshah_coaching_class.Areas.Admin.Models.events_admin> lst1 = new List<jinalshah_coaching_class.Areas.Admin.Models.events_admin>();

                                foreach (var item in q1)
                                {
                                    lst1.Add(new jinalshah_coaching_class.Areas.Admin.Models.events_admin() { event_date = item.event_date_.ToString(), event_name = item.event_name, event_time = item.event_time.ToString(), event_img = item.event_img, event_desc = item.event_desc, event_place = item.event_place });
                                }

                                List<jinalshah_coaching_class.Areas.Admin.Models.events_admin> lst2 = new List<jinalshah_coaching_class.Areas.Admin.Models.events_admin>();

                             
                                for (int i = 0; i < 4; i++)
                                {
                                    lst2.Add(new jinalshah_coaching_class.Areas.Admin.Models.events_admin() { pk_event_id = lst1[i].pk_event_id, event_date = lst1[i].event_date.ToString(), event_name = lst1[i].event_name, event_time = lst1[i].event_time.ToString(), event_img = lst1[i].event_img, event_desc = lst1[i].event_desc, event_place = lst1[i].event_place });
                                }
                            }
                            

                            <section class="widget has-divider">
                                <h3 class="title">Upcoming Events</h3>
                                @foreach(var item in lst2){
                                <article class="events-item row page-row">
                                    <div class="date-label-wrapper col-md-3 col-sm-4 col-xs-4">
                                         @{
                              DateTime d = Convert.ToDateTime(item.event_date);
                              int m = d.Month;
                              int d1 = d.Day;
                              string x = "";
                              switch (m)
                              {
                                  case 1:x="Jan";
                                      break;

                                  case 2: x = "Feb";
                                      break;

                                  case 3: x = "Mar";
                                      break;

                                  case 4: x = "Apr";
                                      break;

                                  case 5: x = "May";
                                      break;

                                  case 6: x = "Jun";
                                      break;

                                  case 7: x = "Jul";
                                      break;

                                  case 8: x = "Aug";
                                      break;

                                  case 9: x = "Sep";
                                      break;

                                  case 10: x = "Oct";
                                      break;

                                  case 11: x = "Nov";
                                      break;

                                  case 12: x = "Dec";
                                      break;
                              }
                                    }
                                 
                                        <p class="date-label">
                                            <span class="month">@x</span>
                                            <span class="date-number">@d1</span>
                                        </p>
                                    </div>
                                    <!--//date-label-wrapper-->
                                    <div class="details col-md-9 col-sm-8 col-xs-8">
                                        <h5 class="title"><a href="@Url.Action("All_events","Home")">@item.event_name</a></h5>
                                        <p class="time text-muted">@item.event_time<br>
                                            @item.event_place</p>
                                    </div>
                                    <!--//details-->
                                </article>
                                }
